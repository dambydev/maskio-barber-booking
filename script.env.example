# Database connection string per gli script di utility e test
DATABASE_URL=postgres://username:password@host/database

# Google OAuth Credentials (necessarie per alcuni script che testano autenticazione)
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret

# Google Places API (per le recensioni)
GOOGLE_PLACES_API_KEY=your-google-places-api-key
GOOGLE_PLACE_ID=your-google-place-id

# NextAuth (per test di autorizzazione)
NEXTAUTH_SECRET=your-auth-secret
NEXTAUTH_URL=http://localhost:3000
